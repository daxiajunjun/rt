{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": false,
      "module": "es2015",
      "target": "ES2015",
      "jsx": "react",
      "lib": ["es2015", "dom", "es2015.symbol"]
  },
  "include": [
      "./web/src/**/*"
  ],
  "awesomeTypescriptLoaderOptions": {
      "reportFiles": [
          "./web/src/**/*"
      ]
  }
}
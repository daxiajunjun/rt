{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": false,
      "module": "commonjs",
      "target": "ES2016",
      "jsx": "react",
      "lib": ["es2015", "dom", "es2017", "es2016"]
  },
  "include": [
      "./web/src/**/*", "web/src/declarations.d.ts"
  ],
  "awesomeTypescriptLoaderOptions": {
      "reportFiles": [
          "./web/src/**/*"
      ]
  }
}